{
  "expo": {
    "name": "ZenFlow",
    "slug": "zenflow",
    "scheme": "zenflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/zenflowimg.png",
    "userInterfaceStyle": "automatic",

    "splash": {
      "image": "./assets/images/zenflowimg.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/zenflowimg.png",
        "backgroundColor": "#000000"
      }
    },

    "web": {
      "favicon": "./assets/images/zenflowimg.png"
    },

    "plugins": []
  }
}
